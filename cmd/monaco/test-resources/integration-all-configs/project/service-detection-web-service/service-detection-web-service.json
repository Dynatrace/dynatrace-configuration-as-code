{
  "type": "FULL_WEB_SERVICE",
  "name": "{{ .name }}",
  "description": "REST API example",
  "enabled": true,
  "detectAsWebRequestService": false,
  "managementZones": [],
  "conditions": [
    {
      "attributeType": "APPLICATION_ID",
      "compareOperations": [
        {
          "type": "STRING_CONTAINS",
          "invert": "false",
          "ignoreCase": "false",
          "values": [
            "value1",
            "value2"
          ]
        }
      ]
    }
  ],
  "webServiceName": {
    "valueOverride": "abc"
  },
  "webServiceNameSpace": {
    "valueOverride": "abc"
  },
  "applicationId": {
    "valueOverride": "abc"
  },
  "contextRoot": {
    "segmentsToCopyFromUrlPath": 2,
    "transformations": [
      {
        "type": "BEFORE",
        "delimiter": "/"
      }
    ]
  },
  "serverName": {
    "transformations": [
      {
        "type": "BEFORE",
        "delimiter": "-"
      }
    ]
  }
}
{
  "name": "{{ .name }}",
  "enabled": true,
  "rules": [
    {
      "enabled": true,
      "fileName": "phpFile",
      "fileNameMatcher": "ENDS_WITH",
      "className": "",
      "matcher": "EQUALS",
      "methodRules": [
        {
          "methodName": "myMethod",
          "returnType": "void",
          "visibility": "PUBLIC"
        }
      ],
      "annotations": []
    }
  ],
  "queueEntryPoint": false,
  "queueEntryPointType": null,
  "processGroups": []
}